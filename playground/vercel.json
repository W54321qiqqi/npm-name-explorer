{
  "rewrites": [
    {
      "source": "/api",
      "destination": "https://npm-name-explorer-api.vercel.app"
    },
    {
      "source": "/api/",
      "destination": "https://npm-name-explorer-api.vercel.app/"
    },
    {
      "source": "/api/:match*",
      "destination": "https://npm-name-explorer-api.vercel.app/:match*"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}
